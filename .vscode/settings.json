{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "yaml.schemas": {
    "https://squidfunk.github.io/mkdocs-material/schema.json": "mkdocs.yml"
  },
  "yaml.customTags": [
    "!ENV scalar",
    "!ENV sequence",
    "!relative scalar",
    "tag:yaml.org,2002:python/name:material.extensions.emoji.to_svg",
    "tag:yaml.org,2002:python/name:material.extensions.emoji.twemoji",
    "tag:yaml.org,2002:python/name:pymdownx.superfences.fence_code_format",
    "tag:yaml.org,2002:python/object/apply:pymdownx.slugs.slugify mapping"
  ],
  "go.addTags": {
    "tags": "json,url,header",
    "promptForTags": true
  },
  "go.coverOnSave": true,
  "go.formatFlags": ["-s", "-w"],
  "go.buildFlags": ["-tags=preview.query"],
  "go.lintOnSave": "package",
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [],
  "cSpell.words": [
    "APIV",
    "attachmentapi",
    "Batchable",
    "batchapi",
    "cmdb",
    "covermode",
    "coverpkg",
    "coverprofile",
    "covignore",
    "dateonly",
    "devcontainers",
    "donjayamanne",
    "dupl",
    "dvcm",
    "Errorable",
    "errorlens",
    "esbenp",
    "Expt",
    "findstr",
    "formserialization",
    "gitblame",
    "githistory",
    "gocognit",
    "golangci",
    "gopath",
    "Gruntfuggly",
    "Headerable",
    "httpheader",
    "jsonserialization",
    "kiota",
    "maint",
    "michaeldcanady",
    "mkdocs",
    "mozillazg",
    "msyrus",
    "multipartserialization",
    "nethttp",
    "nethttplibrary",
    "nocov",
    "nolint",
    "outliner",
    "Paramters",
    "Requestable",
    "Responseable",
    "servicenowsdkgo",
    "strct",
    "stretchr",
    "stylecheck",
    "sysid",
    "sysparm",
    "tableapi",
    "textserialization",
    "timeonly",
    "typedheaders",
    "Unmarshall",
    "unparam",
    "Unserviced",
    "usernamehw",
    "userns",
    "vscodeintellicode",
    "waderyan",
    "yzhang"
  ],
  "gopls": {
    "build.buildFlags": ["-tags=preview.query"]
  },
  "go.useLanguageServer": true
}
